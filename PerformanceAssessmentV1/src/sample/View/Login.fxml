<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.LoginController">
   <children>
      <Label fx:id="LoginUsernameLabel" layoutX="73.0" layoutY="50.0" text="UserName" />
      <Label fx:id="LoginPasswordLabel" layoutX="76.0" layoutY="92.0" text="Password" />
      <TextField fx:id="LoginUserNameTextField" layoutX="159.0" layoutY="46.0" />
      <TextField fx:id="LoginPasswordTextField" layoutX="159.0" layoutY="88.0" />
      <Button fx:id="LoginLoginButton" layoutX="210.0" layoutY="132.0" mnemonicParsing="false" onAction="#loginButtonHandler" text="Login" />
      <Button fx:id="LoginExitButton" layoutX="308.0" layoutY="132.0" mnemonicParsing="false" onAction="#exitButtonHandler" text="Exit" />
      <Label fx:id="LoginLocationDisplayer" layoutX="78.0" layoutY="136.0" prefHeight="17.0" prefWidth="96.0" />
      <Label fx:id="LoginLocationLabel" layoutX="25.0" layoutY="136.0" text="Location:" />
   </children>
</AnchorPane>
